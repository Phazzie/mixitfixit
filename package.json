    "test": "jest",
    "track-errors": "bash scripts/track_typescript_errors.sh",
    "test-track": "bash scripts/test_and_track.sh",
    "docs": "jsdoc -c jsdoc.json",
    "dev:client": "cd client && npm run dev",
    "dev:server": "cd server && npm run dev",
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "install:all": "npm install && cd client && npm install && cd ../server && npm install",
    "validate:all": "npm-run-all validate:*",
    "validate:typescript": "ts-node scripts/validators/validate-typescript.ts",
    "validate:solid": "ts-node scripts/validators/validate-solid.ts",
    "validate:pipeline": "ts-node scripts/validators/validate-pipeline.ts",
    "generate:tests": "ts-node scripts/generate-tests.ts",
    "test:generated": "npm run generate:tests && jest",
    "generate:docs": "ts-node scripts/generators/docs-generator.ts",
    "pre-commit": "npm-run-all validate:* lint test",
    "pre-push": "npm-run-all validate:* test build",
    "watch": "npm-run-all --parallel watch:*",
    "watch:tests": "jest --watch",
    "watch:typescript": "tsc --watch",
    "analyze": "npm-run-all analyze:*",
    "analyze:complexity": "ts-node scripts/analyzers/complexity-analyzer.ts",
    "analyze:coverage": "jest --coverage",
    "validate": "ts-node scripts/validate.ts",
    "validate:structure": "ts-node scripts/validate.ts structure",
    "validate:structure:fix": "ts-node scripts/validate.ts structure --fix",
    "validate:watch": "nodemon --exec npx ts-node ./scripts/validate.ts structure --ext ts,tsx --watch src",
    "issues": "ts-node scripts/manage-issues.ts list",
    "issues:resolve": "ts-node scripts/manage-issues.ts resolve",
    "issues:archive": "ts-node scripts/manage-issues.ts archive",
    "dev-tools": "ts-node scripts/dev-tools.ts",
    "track-move": "ts-node scripts/dev-tools.ts track-move",
    "update-move": "ts-node scripts/dev-tools.ts update-status",
    "analyze-scripts": "ts-node scripts/dev-tools.ts analyze-scripts",
    "validate-all": "ts-node scripts/dev-tools.ts validate"
  },
